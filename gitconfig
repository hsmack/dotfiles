[alias]
	llog = log --date=local
	cl= config --list
	st = status
	br = branch
	ci = commit
	co = checkout
	g = log --graph --simplify-by-decoration --pretty=format:'%d' 
    gl = !git g --all
    gg = !git g --all
    l = log --graph --pretty=format:'%C(yellow)%h%Creset%C(blue)%d%Creset %C(white bold)%s%Creset %C(white dim)(by %an %ar)%Creset'
    ll = !git l --all
